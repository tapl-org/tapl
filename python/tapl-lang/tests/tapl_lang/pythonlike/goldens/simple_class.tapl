language pythonlike

from tapl_lang.lib import tapl_dev

class SimplestClass:
  pass

def accept(param: SimplestClass!):
  pass

accept(SimplestClass())

class Circle:
  def __init__(self, radius: Float):
    self.radius = radius

  def area(self) -> Float:
    return 3.14 * self.radius * self.radius
  
  def set_radius(self, radius: Float):
    self.radius = radius
  
  def is_bigger_than(self, radius: Float):
    return self.radius > radius

def print_area(circle: Circle!):
  tapl_dev.log(circle.area())
  tapl_dev.log(circle.is_bigger_than(1.0))

print_area(Circle(2.0))
