language pythonlike

def simple_id(a):
    return a

tapl_dev.print(simple_id(3))

def poly_id(A):
    def typed_id(a: A):
        return a
    return typed_id
idInt = poly_id(<None:Int>)
idStr = poly_id(<None:Str>)

tapl_dev.print(idInt(3))
tapl_dev.print(tapl_dev.to_string(idStr))
tapl_dev.print(idStr('abc'))
