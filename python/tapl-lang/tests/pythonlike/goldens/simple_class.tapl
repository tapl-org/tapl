class Circle:
  def __init__(self, radius: Float):
    self.radius = radius

  def area(self) -> Float:
    return 3.14 * self.radius * self.radius

def print_area(circle: Circle):
  tapl_dev.print(circle.area())

print_area(Circle_(2.0))
